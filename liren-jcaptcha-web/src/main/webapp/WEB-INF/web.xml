<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>jcaptcha</servlet-name>
		<servlet-class>com.octo.captcha.servlet.image.SimpleImageCaptchaServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>JcaptchaServlet</servlet-name>
		<display-name>JcaptchaServlet</display-name>
		<description></description>
		<servlet-class>org.liren.jcaptcha.JcaptchaServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>jcaptcha</servlet-name>
		<url-pattern>/jcaptcha.jpg</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>JcaptchaServlet</servlet-name>
		<url-pattern>/JcaptchaServlet</url-pattern>
	</servlet-mapping>
	
</web-app>
